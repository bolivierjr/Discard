{
  "name": "discard-server",
  "version": "1.0.0",
  "engines": {
    "node": ">=11.5"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "npm-run-all": "^4.1.5",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.10"
  },
  "scripts": {
    "start:server": "concurrently \"yarn server\" \"yarn client\"",
    "start:dev": "NODE_ENV=development nodemon --inspect index.js",
    "build:server": "babel -d ./build ./server -s --presets=env",
    "build": "yarn build:client && yarn build:server",
    "start:prod": "NODE_ENV=production node build/index.js"
  }
}
