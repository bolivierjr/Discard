{
  "name": "discard",
  "version": "0.1.0",
  "engines": {
    "node": ">=10.6.0"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "redis": "^2.8.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.3"
  },
  "scripts": {
    "start:dev": "concurrently \"yarn server\" \"yarn client\"",
    "server": "NODE_ENV=development nodemon --inspect server/index.js --exec babel-node --presets=env",
    "client": "react-scripts start",
    "build:server": "babel -d ./build ./server -s --presets=env",
    "build:client": "react-scripts build",
    "build": "yarn build:client && yarn build:server",
    "start:prod": "NODE_ENV=production node build/index.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:8080"
}
