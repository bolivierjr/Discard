{
  "name": "discard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.3"
  },
  "scripts": {
    "start": "NODE_ENV=production node build/index.js",
    "start:dev": "concurrently \"yarn server\" \"yarn client\"",
    "server": "NODE_ENV=development nodemon --inspect server/index.js --exec babel-node --presets env",
    "client": "react-scripts start",
    "build:server": "babel -d ./build ./server -s --presets env",
    "build": "react-scripts build && yarn build:server",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:8080"
}
